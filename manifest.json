{
  "manifest_version": 2,
  "default_locale": "en",
  "name": "SF Skip Download Countdown",
  "description": "__MSG_extensionDescription__",
  "author": "Andreas Bielawski",
  "version": "2.0",
  "icons" : {
    "32" : "icons/32.png",
    "48" : "icons/48.png"
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://sourceforge.net/projects/*/files/*/download*"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}